{
  "main": "src/index.html",
  "scripts": {
    "prebuild": "rm -rf docs",
    "build": "parcel build --no-minify src/index.html  --out-dir docs --public-url ./",
    "predev": "rm -rf docs",
    "dev": "parcel src/index.html --out-dir docs",
    "deploy": "npm run build && git add . && git commit -a -m 'latest build' && git push origin master"
  },
  "staticFiles": {
    "staticPath": "src/static",
    "watcherGlob": "**"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "include": "^1.1.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.3.1",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "posthtml-include": "^1.4.0"
  },
  "name": "phonebot"
}
